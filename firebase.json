{
  "database": {
    "rules": "database.rules.json"
  },
  "storage": {
    "rules": "storage.rules.json"
  },
  "functions": [
    {
      "source": "functions",
      "runtime": "nodejs18",
      "codebase": "default"
    }
  ],
  "hosting": {
    "public": "hosting_public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/electrical/generateBoq",
        "function": "electricalGenerateBoq"
      }
    ]
  }
}